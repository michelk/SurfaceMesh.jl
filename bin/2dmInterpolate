#!/usr/bin/env julia
# | Interpolate node-z-values from another mesh
if (ARGS in ["-h", "--help"]) || length(ARGS) == 0
    print("Usage 2dmInterpolate toInt.2dm from.2dm")
    exit()
end

using SurfaceMesh
ms = map(read2dm, ARGS)
m = SmsMesh(interpolateZ(ms[1].msh, ms[2].msh), ms[1].frs, ms[1].bcNds)
write2dm(m)
